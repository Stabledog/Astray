    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "pwa-chrome",
            "urlFilter":"http://localhost:5500/*",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Launch Chrome against localhost",
            "type": "chrome",
            "port": 9222,
            "request": "launch",
            "url": "http://localhost:5500/Astray/index.html",
            "webRoot": "${workspaceFolder}",
            "trace": true
        }
    ]
}
